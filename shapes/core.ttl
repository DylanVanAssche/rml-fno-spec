<http://schema.org/RMLLanguageMapShape> a sh:NodeShape ;
    sh:description """
    Represents a Language Map.
    """ ;
    sh:message """
    rml:LanguageMap must specify an rml:template or rml:constant with the IRI 
    of the language.
    """ ;
    sh:name "LanguageMap" ;
    sh:property [ sh:description """
        Exactly one rml:template or one rml:reference or one rml:constant
        is required.
        """ ;
            sh:maxCount 1 ;
            sh:message """
        Exactly one rml:template or one rml:reference or one rml:constant
        is required.
        """ ;
            sh:minCount 1 ;
            sh:name "template/reference/constant" ;
            sh:path [ sh:alternativePath ( rml:template rml:reference rml:constant ) ] ],
        [ sh:description """
        A logical target is any target to where generated RDF triples are
        exported to.
        """ ;
            sh:maxCount 1 ;
            sh:message """
        Zero or one rml:logicalTarget is required to export RDF triples.
        """ ;
            sh:minCount 0 ;
            sh:name "logicalTarget" ;
            sh:nodeKind sh:BlankNodeOrIRI ;
            sh:path rml:logicalTarget ],
        [ sh:datatype xsd:string ;
            sh:description """
        A template (format string) to specify how to generate a value for a 
        subject, predicate, or object, using one or more columns from a logical
        table row or iterator.
        """ ;
            sh:message """
        rml:template must be a string.
        """ ;
            sh:name "template" ;
            sh:nodeKind sh:Literal ;
            sh:path rml:template ],
        [ sh:datatype xsd:string ;
            sh:description """
        A reference rml:reference is used to refer to a column in case of
        databases, a record in case of CSV or TSV data source, an element in
        case of XML data source, an object in case of a JSON data source, etc.

        A reference must be a valid identifier, considering the reference
        formulation (rml:referenceFormulation) specified. The reference can be
        an absolute path, or a path relative to the iterator specified at the
        logical source.   
        """ ;
            sh:message """
        rml:reference must be a string.
        """ ;
            sh:name "reference" ;
            sh:nodeKind sh:Literal ;
            sh:path rml:reference ],
        [ sh:description """
        A property for indicating whether a term map is a constant-valued term 
        map.
        """ ;
            sh:message """
        rml:constant must be an IRI.
        """ ;
            sh:name "constant" ;
            sh:nodeKind sh:IRI ;
            sh:path rml:constant ],
        [ sh:description """
        An IRI indicating whether subject or object generated using the 
        value from column name specified should be an IRI reference, blank node,
        or a Literal. 
        """ ;
            sh:in ( rml:Literal ) ;
            sh:maxCount 1 ;
            sh:message """
        rml:termType must be an rml:Literal for a rml:LanguageMap.
        """ ;
            sh:minCount 0 ;
            sh:name "termType" ;
            sh:nodeKind sh:IRI ;
            sh:path rml:termType ] .

<http://schema.org/RMLTriplesMapShape> a sh:NodeShape ;
    sh:description """
    Represents a triples map.
    """ ;
    sh:message """
    rml:TriplesMap requires exactly one rml:subject or one rml:subjectMap and zero
    or more rml:predicateObjectMaps.
    """ ;
    sh:name "TriplesMap" ;
    sh:property [ sh:description """
        A logical source is any source that is mapped to RDF triples.
        """ ;
            sh:maxCount 1 ;
            sh:message """
        Exactly one rml:logicalSource is required to access the data source.
        """ ;
            sh:minCount 1 ;
            sh:name "logicalSource" ;
            sh:nodeKind sh:BlankNodeOrIRI ;
            sh:path rml:logicalSource ],
        [ sh:description """
        Either one rml:subject or one rml:SubjectMap is required, not both.
        """ ;
            sh:maxCount 1 ;
            sh:message """
        Either one rml:subject or one rml:SubjectMap is required, not both.
        """ ;
            sh:minCount 1 ;
            sh:name "subjectMap/subject" ;
            sh:path [ sh:alternativePath ( rml:subjectMap rml:subject ) ] ],
        [ sh:description """
        A SubjectMap element to generate a subject from a logical table row or
        iterator.
        """ ;
            sh:message """
        rml:SubjectMap must be an IRI or blank node.
        """ ;
            sh:name "subjectMap" ;
            sh:node <http://schema.org/RMLSubjectMapShape> ;
            sh:nodeKind sh:BlankNodeOrIRI ;
            sh:path rml:subjectMap ],
        [ sh:description """
        An IRI reference for use as subject for all the RDF triples generated 
        from a logical table row or iterator.
        """ ;
            sh:message """
        rml:subject must be an IRI or blank node.
        """ ;
            sh:name "subject" ;
            sh:node <http://schema.org/RMLsubjectShape> ;
            sh:nodeKind sh:BlankNodeOrIRI ;
            sh:path rml:subject ],
        [ sh:description """
        A PredicateObjectMap element to generate (predicate, object) pair from 
        a logical table row.
        """ ;
            sh:message """
        rml:PredicateObjectMap must be an IRI or blank node.
        """ ;
            sh:minCount 0 ;
            sh:name "predicateObjectMap" ;
            sh:node <http://schema.org/RMLPredicateObjectMapShape> ;
            sh:nodeKind sh:BlankNodeOrIRI ;
            sh:path rml:predicateObjectMap ] ;
    sh:targetClass rml:TriplesMap .

<http://schema.org/RMLdatatypeShape> a sh:NodeShape ;
    sh:description """
    An IRI reference for use as the graph name of all triples generated with 
    the datatype.
    """ ;
    sh:message """
    rml:datatype must be an IRI.
    """ ;
    sh:name "datatype" ;
    sh:nodeKind sh:IRI .

<http://schema.org/RMLlanguageShape> a sh:NodeShape ;
    sh:datatype xsd:string ;
    sh:description """
    Specified the language for the generated Literal.
    """ ;
    sh:message """
    rml:language must be a valid language tag according to BCP47 and may
    only be provided once as string.
    """ ;
    sh:name "language" ;
    sh:pattern "^((?:(en-GB-oed|i-ami|i-bnn|i-default|i-enochian|i-hak|i-klingon|i-lux|i-mingo|i-navajo|i-pwn|i-tao|i-tay|i-tsu|sgn-BE-FR|sgn-BE-NL|sgn-CH-DE)|(art-lojban|cel-gaulish|no-bok|no-nyn|zh-guoyu|zh-hakka|zh-min|zh-min-nan|zh-xiang))|((?:([A-Za-z]{2,3}(-(?:[A-Za-z]{3}(-[A-Za-z]{3}){0,2}))?)|[A-Za-z]{4})(-(?:[A-Za-z]{4}))?(-(?:[A-Za-z]{2}|[0-9]{3}))?(-(?:[A-Za-z0-9]{5,8}|[0-9][A-Za-z0-9]{3}))*(-(?:[0-9A-WY-Za-wy-z](-[A-Za-z0-9]{2,8})+))*(-(?:x(-[A-Za-z0-9]{1,8})+))?)|(?:x(-[A-Za-z0-9]{1,8})+))$" .

<http://schema.org/RMLObjectMapShape> a sh:NodeShape ;
    sh:description """
    Represents an Object Map.
    """ ;
    sh:name "ObjectMap" ;
    sh:property [ sh:description """
        Exactly one rml:template, one rml:constant or one rml:reference
        is required.
        """ ;
            sh:maxCount 1 ;
            sh:message """
        Exactly one rml:template, one rml:constant or one rml:reference is
        required.
        """ ;
            sh:minCount 1 ;
            sh:name "template/constant/reference" ;
            sh:path [ sh:alternativePath ( rml:template rml:constant rml:reference ) ] ],
        [ sh:description """
        rml:language and rml:datatype may not be provided at the same time.
        """ ;
            sh:maxCount 1 ;
            sh:minCount 0 ;
            sh:name "languageMap/datatypeMap" ;
            sh:path [ sh:alternativePath ( rml:languageMap rml:datatypeMap ) ] ],
        [ sh:description """
        Specified the language map for the object component for the generated 
        triple from a logical table row or iterator.
        """ ;
            sh:maxCount 1 ;
            sh:message """
        rml:languageMap must point to a rml:LanguageMap specifying the language
        of the string Literal.
        """ ;
            sh:minCount 0 ;
            sh:name "languageMap" ;
            sh:node <http://schema.org/RMLDatatypeMapShape> ;
            sh:nodeKind sh:BlankNodeOrIRI ;
            sh:path rml:languageMap ],
        [ sh:description """
        Specifies the datatype of the object component for the generated triple
        from a logical table row or iterator.
        """ ;
            sh:maxCount 1 ;
            sh:message """
        rml:datatypeMap must point to a rml:DatatypeMap specifying the datatype
        of the Literal.
        """ ;
            sh:minCount 0 ;
            sh:name "datatypeMap" ;
            sh:node <http://schema.org/RMLDatatypeMapShape> ;
            sh:nodeKind sh:BlankNodeOrIRI ;
            sh:path rml:datatypeMap ],
        [ sh:description """
        A logical target is any target to where generated RDF triples are
        exported to.
        """ ;
            sh:maxCount 1 ;
            sh:message """
        Zero or one rml:logicalTarget is required to export RDF triples.
        """ ;
            sh:minCount 0 ;
            sh:name "logicalTarget" ;
            sh:nodeKind sh:BlankNodeOrIRI ;
            sh:path rml:logicalTarget ],
        [ sh:datatype xsd:string ;
            sh:description """
        A template (format string) to specify how to generate a value for a 
        subject, predicate, or object, using one or more columns from a logical
        table row or iterator.
        """ ;
            sh:message """
        rml:template must be a string.
        """ ;
            sh:name "template" ;
            sh:nodeKind sh:Literal ;
            sh:path rml:template ],
        [ sh:description """
        A property for indicating whether a term map is a constant-valued term 
        map.
        """ ;
            sh:name "constant" ;
            sh:path rml:constant ],
        [ sh:datatype xsd:string ;
            sh:description """
        A reference rml:reference is used to refer to a column in case of 
        databases, a record in case of CSV or TSV data source, an element in 
        case of XML data source, an object in case of a JSON data source, etc.

        A reference must be a valid identifier, considering the reference 
        formulation (rml:referenceFormulation) specified. The reference can be
        an absolute path, or a path relative to the iterator specified at the 
        logical source. 
        """ ;
            sh:message """
        rml:reference must be a string.
        """ ;
            sh:name "reference" ;
            sh:nodeKind sh:Literal ;
            sh:path rml:reference ],
        [ sh:description """
        An IRI indicating whether subject or object generated using the 
        value from column name specified should be an IRI reference, blank node,
        or a Literal. 
        """ ;
            sh:in ( rml:IRI rml:BlankNode rml:Literal ) ;
            sh:maxCount 1 ;
            sh:message """
        rml:termType must be either rml:IRI, rml:BlankNode or rml:Literal for
        a rml:ObjectMap.
        """ ;
            sh:minCount 0 ;
            sh:name "termType" ;
            sh:nodeKind sh:IRI ;
            sh:path rml:termType ] .

<http://schema.org/RMLPredicateMapShape> a sh:NodeShape ;
    sh:description """
    Represents a Predicate Map.
    """ ;
    sh:name "PredicateMap" ;
    sh:property [ sh:description """
        Exactly one rml:template, one rml:constant or one rml:reference is
        required.
        """ ;
            sh:maxCount 1 ;
            sh:message """
        Exactly one rml:template, one rml:constant or one rml:reference is
        required.
        """ ;
            sh:minCount 1 ;
            sh:name "template/constant/reference" ;
            sh:path [ sh:alternativePath ( rml:template rml:constant rml:reference ) ] ],
        [ sh:description """
        A logical target is any target to where generated RDF triples are
        exported to.
        """ ;
            sh:maxCount 1 ;
            sh:message """
        Zero or one rml:logicalTarget is required to export RDF triples.
        """ ;
            sh:minCount 0 ;
            sh:name "logicalTarget" ;
            sh:nodeKind sh:BlankNodeOrIRI ;
            sh:path rml:logicalTarget ],
        [ sh:datatype xsd:string ;
            sh:description """
        A template (format string) to specify how to generate a value for a 
        subject, predicate, or object, using one or more columns from a logical
        table row or iterator.
        """ ;
            sh:message """
        rml:template must be a string.
        """ ;
            sh:name "template" ;
            sh:nodeKind sh:Literal ;
            sh:path rml:template ],
        [ sh:description """
        A property for indicating whether a term map is a constant-valued term 
        map.
        """ ;
            sh:message """
        rml:constant must be an IRI.
        """ ;
            sh:name "constant" ;
            sh:nodeKind sh:IRI ;
            sh:path rml:constant ],
        [ sh:datatype xsd:string ;
            sh:description """
        A reference rml:reference is used to refer to a column in case of 
        databases, a record in case of CSV or TSV data source, an element in 
        case of XML data source, an object in case of a JSON data source, etc.

        A reference must be a valid identifier, considering the reference 
        formulation (rml:referenceFormulation) specified. The reference can be
        an absolute path, or a path relative to the iterator specified at the 
        logical source. 
        """ ;
            sh:message """
        rml:reference must be a string.
        """ ;
            sh:name "reference" ;
            sh:nodeKind sh:Literal ;
            sh:path rml:reference ],
        [ sh:description """
        An IRI indicating whether subject or object generated using the value 
        from column name specified should be an IRI reference, blank node,
        or a Literal.
        """ ;
            sh:in ( rml:IRI ) ;
            sh:maxCount 1 ;
            sh:message """
        rml:termType must be either rml:IRI for an rml:PredicateMap.
        May only be provided once.
        """ ;
            sh:name "termType" ;
            sh:nodeKind sh:IRI ;
            sh:path rml:termType ] .

<http://schema.org/RMLPredicateObjectMapShape> a sh:NodeShape ;
    sh:description """
    Represents a Predicate Object Map.
    """ ;
    sh:name "PredicateObjectMap" ;
    sh:property [ sh:description """
        Either an rml:predicate or rml:predicateMap must be provided, not both.
        """ ;
            sh:message """
        Either an rml:predicate or rml:predicateMap must be provided, not both.
        """ ;
            sh:minCount 1 ;
            sh:name "predicate/predicateMap" ;
            sh:path [ sh:alternativePath ( rml:predicate rml:predicateMap ) ] ],
        [ sh:description """
        An ObjectMap element to generate the object component of the 
        (predicate, object) pair from a logical table row or iterator.
        """ ;
            sh:message """
        rml:objectMap must be an IRI or blank node and be provided once.
        """ ;
            sh:name "objectMap" ;
            sh:nodeKind sh:BlankNodeOrIRI ;
            sh:path rml:objectMap ;
            sh:xone ( [ sh:node <http://schema.org/RMLObjectMapShape> ] [ sh:node <http://schema.org/RMLRefObjectMapShape> ] ) ],
        [ sh:description """
        Specifies the object for the generated triple from the logical table 
        row or iterator.
        """ ;
            sh:message """
        rml:object must be an IRI and be provided once.
        """ ;
            sh:name "object" ;
            sh:node <http://schema.org/RMLobjectShape> ;
            sh:nodeKind sh:IRI ;
            sh:path rml:object ],
        [ sh:description """
        Either an rml:graph or rml:graphMap may be optionally be provided, but 
        not both.
        """ ;
            sh:maxCount 1 ;
            sh:message """
        Either an rml:graph or rml:graphMap may be optionally be provided, but
        not both.
        """ ;
            sh:minCount 0 ;
            sh:name "graph/graphMap" ;
            sh:path [ sh:alternativePath ( rml:graph rml:graphMap ) ] ],
        [ sh:description """
        Specifies a GraphMap. When used with a SubjectMap element, all the RDF 
        triples generated from a logical row will be stored in the specified 
        named graph. Otherwise, the RDF triple generated using the 
        (predicate, object) pair will be stored in the specified named graph.
        """ ;
            sh:message """
        rml:graphMap must be either an IRI or blank node. 
        """ ;
            sh:name "graphMap" ;
            sh:node <http://schema.org/RMLGraphMapShape> ;
            sh:nodeKind sh:BlankNodeOrIRI ;
            sh:path rml:graphMap ],
        [ sh:description """
        An IRI reference for use as the graph name of all triples generated 
        with the GraphMap.
        """ ;
            sh:message """
        rml:graphMap must be an IRI. 
        """ ;
            sh:name "graph" ;
            sh:node <http://schema.org/RMLgraphShape> ;
            sh:nodeKind sh:IRI ;
            sh:path rml:graph ],
        [ sh:description """
        A PredicateMap element to generate the predicate component of the 
        (predicate, object) pair from a logical table row or iterator.
        """ ;
            sh:message """
        rml:predicateMap must be an IRI or blank node and be provided once.
        """ ;
            sh:name "predicateMap" ;
            sh:node <http://schema.org/RMLPredicateMapShape> ;
            sh:nodeKind sh:BlankNodeOrIRI ;
            sh:path rml:predicateMap ],
        [ sh:description """
        Specifies the predicate for the generated triple from the logical table 
        row or iterator.
        """ ;
            sh:message """
        rml:predicate must be an IRI and be provided once.
        """ ;
            sh:name "predicate" ;
            sh:node <http://schema.org/RMLpredicateShape> ;
            sh:nodeKind sh:IRI ;
            sh:path rml:predicate ],
        [ sh:description """
        Either an rml:object or rml:objectMap must be provided, not both.
        """ ;
            sh:maxCount 1 ;
            sh:message """
        Either an rml:object or rml:objectMap must be provided, not both.
        """ ;
            sh:minCount 1 ;
            sh:name "object/objectMap" ;
            sh:path [ sh:alternativePath ( rml:object rml:objectMap ) ] ] .

<http://schema.org/RMLRefObjectMapShape> a sh:NodeShape ;
    sh:description """
    Represents a Reference Object Map.
    """ ;
    sh:message """
    rml:RefObjectMap must specify a rml:parentTriplesMap and zero or more 
    rml:joinConditions.
    """ ;
    sh:name "RefObjectMap" ;
    sh:property [ sh:class <http://www.w3.org/ns/r2rml#TriplesMap> ;
            sh:description """
        Specifies the TriplesMap element corresponding to the parent logical 
        table of the foreign key constraint.
        """ ;
            sh:maxCount 1 ;
            sh:message """
        rml:parentTriplesMap may only be provided once and must be an IRI or 
        blank node referring to a rml:TriplesMap.
        """ ;
            sh:minCount 1 ;
            sh:name "parentTriplesMap" ;
            sh:nodeKind sh:BlankNodeOrIRI ;
            sh:path <http://www.w3.org/ns/r2rml#parentTriplesMap> ],
        [ sh:description """
        Specifies the join condition for joining the child logical table with 
        the parent logical table of the foreign key constraint.
        """ ;
            sh:message """
        rml:joinCondition must specify an rml:parent and rml:child.
        """ ;
            sh:name "joinCondition" ;
            sh:nodeKind sh:BlankNodeOrIRI ;
            sh:path <http://www.w3.org/ns/r2rml#joinCondition> ;
            sh:property [ sh:datatype xsd:string ;
                    sh:description """
            Names a column in the parent table of a join.
            """ ;
                    sh:maxCount 1 ;
                    sh:message """
            rml:parent must be string and only occur once.
            """ ;
                    sh:minCount 1 ;
                    sh:name "parent" ;
                    sh:nodeKind sh:Literal ;
                    sh:path <http://www.w3.org/ns/r2rml#parent> ],
                [ sh:datatype xsd:string ;
                    sh:description """
            Names a column in the child table of a join.
            """ ;
                    sh:maxCount 1 ;
                    sh:message """
            rml:child must be string and only occur once.
            """ ;
                    sh:minCount 1 ;
                    sh:name "child" ;
                    sh:nodeKind sh:Literal ;
                    sh:path <http://www.w3.org/ns/r2rml#child> ] ] .

<http://schema.org/RMLSubjectMapShape> a sh:NodeShape ;
    sh:description """
    Represents a Subject Map.
    """ ;
    sh:message """
    rml:subjectMap requires one rml:template or one rml:constant or one
    rml:reference.
    """ ;
    sh:name "SubjectMap" ;
    sh:property [ sh:description """
        Exactly one rml:template, one rml:constant or one rml:reference is
        required.
        """ ;
            sh:maxCount 1 ;
            sh:message """
        Exactly one rml:template, one rml:constant or one rml:reference is
        required.
        """ ;
            sh:minCount 1 ;
            sh:name "template/constant/reference" ;
            sh:path [ sh:alternativePath ( rml:template rml:constant rml:reference ) ] ],
        [ sh:description """
        An IRI indicating whether subject or object generated using the value 
        from column name specified for rml:column should be an IRI reference, 
        blank node, or a literal.
        """ ;
            sh:in ( rml:IRI rml:BlankNode ) ;
            sh:maxCount 1 ;
            sh:message """
        rml:termType must be either rml:IRI or rml:BlankNode for an rml:SubjectMap.
        May only be provided once.
        """ ;
            sh:name "termType" ;
            sh:nodeKind sh:IRI ;
            sh:path rml:termType ],
        [ sh:description """
        Either an rml:graph or rml:graphMap may be optionally be provided, but 
        not both.
        """ ;
            sh:maxCount 1 ;
            sh:message """
        Either an rml:graph or rml:graphMap may be optionally be provided, but
        not both.
        """ ;
            sh:minCount 0 ;
            sh:name "graph/graphMap" ;
            sh:path [ sh:alternativePath ( rml:graph rml:graphMap ) ] ],
        [ sh:description """
        Specifies a GraphMap. When used with a SubjectMap element, all the RDF 
        triples generated from a logical row will be stored in the specified 
        named graph. Otherwise, the RDF triple generated using the 
        (predicate, object) pair will be stored in the specified named graph.
        """ ;
            sh:message """
        rml:graphMap must be either an IRI or blank node. 
        """ ;
            sh:name "graphMap" ;
            sh:node <http://schema.org/RMLGraphMapShape> ;
            sh:nodeKind sh:BlankNodeOrIRI ;
            sh:path rml:graphMap ],
        [ sh:description """
        An IRI reference for use as the graph name of all triples generated 
        with the GraphMap.
        """ ;
            sh:message """
        rml:graphMap must be an IRI. 
        """ ;
            sh:name "graph" ;
            sh:node <http://schema.org/RMLgraphShape> ;
            sh:nodeKind sh:IRI ;
            sh:path rml:graph ],
        [ sh:description """
        A logical target is any target to where generated RDF triples are
        exported to.
        """ ;
            sh:maxCount 1 ;
            sh:message """
        Zero or one rml:logicalTarget is required to export RDF triples.
        """ ;
            sh:minCount 0 ;
            sh:name "logicalTarget" ;
            sh:nodeKind sh:BlankNodeOrIRI ;
            sh:path rml:logicalTarget ],
        [ sh:datatype xsd:string ;
            sh:description """
        A template (format string) to specify how to generate a value for a 
        subject, predicate, or object, using one or more columns from a logical
